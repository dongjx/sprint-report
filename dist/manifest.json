{
  "name": "Spring report chrome extension",
  "version": "1.0",
  "description": "Spring report ",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "background",
    "*://*.google.com/",
    "notifications",
    "storage"
  ],
  "browser_action": {
    "default_title": "Spring Report",
    "default_icon": {
      "16": "logo-64.png",
      "32": "logo-128.png",
      "64": "logo-256.png"
    }
  },
  "options_page": "index.html",
  "background": {
    "scripts": [
     "background.js"
    ]
  },
  "manifest_version": 2
}
